{
  "name": "typedoc-theme-oxide",
  "version": "0.1.3",
  "description": "A TypeDoc theme inspired by rustdoc.",
  "main": "dist/index.js",
  "repository": "github:balthild/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "clean-css": "^5.3.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3",
    "undici": "^5.5.1"
  },
  "peerDependencies": {
    "typedoc": "^0.22.17"
  },
  "scripts": {
    "build": "node scripts/retrieve-assets.js && rm -rf dist && tsc",
    "predocs": "tsc",
    "docs": "typedoc src --plugin ./dist/index.js --theme oxide --out docs",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "assets"
  ]
}
